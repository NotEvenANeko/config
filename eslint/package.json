{
  "name": "@notevenaneko/eslint-config",
  "version": "1.1.1",
  "description": "A personal eslint config.",
  "keywords": [
    "eslint",
    "config"
  ],
  "type": "commonjs",
  "exports": {
    ".": "./dist/index.js",
    "./base": "./dist/base.js",
    "./jest": "./dist/jest.js",
    "./next": "./dist/next.js",
    "./prettier": "./dist/prettier.js",
    "./react": "./dist/react.js",
    "./vue": "./dist/vue.js"
  },
  "homepage": "https://github.com/NotEvenANeko/config",
  "repository": "https://github.com/NotEvenANeko/config",
  "author": "Neko <neko@qwq.icu>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "build": "tsup"
  },
  "dependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint-config-next": "^13.4.12",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-vue": "^9.16.1",
    "vue-eslint-parser": "^9.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.4.5",
    "eslint-define-config": "^1.22.0"
  },
  "peerDependencies": {
    "eslint": ">=8.18.0",
    "prettier": ">=2.7.1",
    "typescript": ">=4.7.4"
  },
  "tsup": {
    "entry": [
      "./src/*.ts"
    ],
    "clean": true,
    "format": [
      "cjs"
    ],
    "outDir": "./dist",
    "sourcemap": false,
    "minify": true
  }
}
