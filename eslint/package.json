{
  "name": "@notevenaneko/eslint-config",
  "version": "1.1.1",
  "description": "A personal eslint config.",
  "keywords": [
    "eslint",
    "config"
  ],
  "type": "commonjs",
  "exports": {
    ".": "./dist/index.js",
    "./base": "./dist/base.js",
    "./jest": "./dist/jest.js",
    "./next": "./dist/next.js",
    "./prettier": "./dist/prettier.js",
    "./react": "./dist/react.js",
    "./vue": "./dist/vue.js",
    "./flat": "./dist/flat/index.js"
  },
  "homepage": "https://github.com/Cattttttttt/config",
  "repository": "https://github.com/Cattttttttt/config",
  "author": "Neko <neko@qwq.icu>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "build": "tsup"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint-config-next": "^12.2.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.8.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-vue": "^9.1.1",
    "vue-eslint-parser": "^9.0.2"
  },
  "devDependencies": {
    "eslint": "^8.18.0",
    "eslint-define-config": "^1.22.0",
    "prettier": "^2.7.1",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "eslint": ">=8.18.0",
    "prettier": ">=2.7.1",
    "typescript": ">=4.7.4"
  },
  "tsup": {
    "entry": [
      "./src/*.ts"
    ],
    "clean": true,
    "format": [
      "cjs"
    ],
    "outDir": "./dist"
  }
}
